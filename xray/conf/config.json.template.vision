    "routing": {
      "rules": [
        {
          "type": "field",
          "outboundTag": "${XRAY_MEDIA_TAG}",
          "domain": [
            "geosite:netflix",
            "geosite:disney"
          ]
        },
        {
          "type": "field",
          "domain": [
              "geosite:cn"
          ],
          "outboundTag": "wireguard"
        },
        {
            "type": "field",
            "ip": [
                "geoip:cn",
                "geoip:private"
            ],
            "outboundTag": "wireguard"
        },
        {
          "type": "field",
          "outboundTag": "common",
          "network": "udp,tcp"
        }
      ]
    },
    "policy": {
      "statsInboundUplink": true,
      "statsInboundDownlink": true,
      "statsOutboundUplink": true,
      "statsOutboundDownlink": true
    },
    "dns": null,
    "transport": null,
    "stats": null,
    "reverse": null,
    "fakeDns": null
