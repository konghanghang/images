        "streamSettings": {
          "network": "tcp"
        }
      }
    ],
    "outbounds": [
      {
        "tag":"common",
        "protocol": "freedom"
      },
      {
        "protocol": "shadowsocks",
        "settings": {
          "servers": [
            {
              "address": "1.1.1.1",
              "port": 13103,
              "method": "chacha20-ietf-poly1305",
              "password": "sspw14250"
            }
          ]
        },
        "streamSettings": {
          "network": "tcp"
        },
        "tag": "netflix-disney"
      },
      {
        "protocol": "wireguard",
        "settings": {
          "secretKey": "我的私钥",
          "address": ["172.16.0.2/32", "2606:4700:110:8949:fed8:2642:a640:c8e1/128"],
          "peers": [
            {
              "publicKey": "Warp公钥",
