                  "certificates": [
                      {
                          "certificateFile": "/ssl/${XRAY_SSL_HOST}/${XRAY_SSL_HOST}.crt",
                          "keyFile": "/ssl/${XRAY_SSL_HOST}/${XRAY_SSL_HOST}.key"
                      }
                  ]
